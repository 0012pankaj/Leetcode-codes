class Solution {
    public boolean increasingTriplet(int[] nums) {
        int n=nums.length;
        if(n<3){
            return false;
        }
        int max1=Integer.MAX_VALUE;
        int max2=Integer.MAX_VALUE;

        for(int num: nums){
           if(num <= max1) max1=num;
           else if(num <= max2) max2=num;
           else{
               return true;
           }
        }
       
       
     return false;   
    }
}
