
class Solution {
    public ListNode middleNode(ListNode head) {
        int count=0;
        ListNode node=new ListNode();
         ListNode node1=new ListNode();
        node=head;
        node1=head;

        int i=0;
        int j=0;

        while(node1 != null && node1.next != null ){
            count++;
            node=node.next;
            node1=node1.next.next;
        }
        
       
       

        return node;
    }
}
